{
  "name": "tourze/aws-lightsail-bundle",
  "description": "Lightsail",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-hash": "*",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-client-contracts": "^2.5 | ^3.0",
    "symfony/http-kernel": "^6.4",
    "tourze/http-client-bundle": "0.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "AwsLightsailBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AwsLightsailBundle\\Tests\\": "tests"
    }
  }
}